{
    "syscalls": {
        "default": "Security",
        "Allow": [
            "access",
            "arch_prctl",
            "brk",
            "clock_getres",
            "clock_gettime",
            "close",
            "dup",
            "dup2",
            "dup3",
            "exit",
            "exit_group",
            "fcntl",
            "futex",
            "getcwd",
            "getdents",
            "getdents64",
            "getegid",
            "geteuid",
            "getpid",
            "getppid",
            "getrandom",
            "getrlimit",
            "getrusage",
            "gettid",
            "gettimeofday",
            "getuid",
            "fstat",
            "ioctl",
            "lseek",
            "madvise",
            "mmap",
            "mprotect",
            "mremap",
            "munmap",
            "nanosleep",
            "read",
            "restart_syscall",
            "rt_sigaction",
            "rt_sigprocmask",
            "rt_sigreturn",
            "sigaction",
            "sigaltstack",
            "sigprocmask",
            "sigreturn",
            "sysinfo",
            "time",
            "write",
            "writev"
        ],
        "Deny": [
            "kill",
            "tkill",
            "tgkill"
        ],
        "CheckPath": [
            "lstat64",
            "lstat",
            "newlstat",
            "newstat",
            "open",
            "openat",
            "stat64",
            "stat",
            "readlink"
        ]
    },
    "paths": {
        "default": "Denied",
        "ReadOnly": [
            "/lib/",
            "/lib32/",
            "/lib64/",
            "/usr/lib/",
            "/usr/lib32/",
            "/usr/lib64/",
            "/usr/libexec/",
            "/usr/local/lib/",
            "/usr/local/lib32/",
            "/usr/local/lib64/",
            "/usr/local/libexec/",
            "/usr/share/",
            "/usr/local/share/",
            "/etc/ld.so.preload",
            "/etc/ld.so.cache",
            "/dev/zero",
            "/dev/null",
            "/dev/tty",
            "/dev/urandom",
            "/etc/passwd",
            "/etc/group",
            "/proc/self/"
        ]
    }
}
